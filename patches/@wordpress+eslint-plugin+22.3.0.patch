+		cosmiconfigSync( 'prettier' ).search() || {};
+	const prettierConfig = { ...defaultPrettierConfig, ...localPrettierConfig };
+	config.push( prettierRecommended, {
+		rules: {
+			'prettier/prettier': [ 'error', prettierConfig ],
