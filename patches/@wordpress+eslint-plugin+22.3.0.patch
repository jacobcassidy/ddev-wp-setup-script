+			// TypeScript may only run on a subset of files. Prefer eslint which is more
+			// likely to run on the entire codebase.
+			'no-unused-vars': 'off',
+			'@typescript-eslint/no-unused-vars': [
+				'error',
+				{ ignoreRestSiblings: true, caughtErrors: 'none' },
